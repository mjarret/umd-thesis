eprint = {1401.0089},
keywords = {adiabatic theorems for,adiabatic theorems with and,and phrases,eral operators and not,for gen-,necessarily,operators with time-independent or,semisimple spectral values,time-dependent domains,weakly,without spectral gap condition},
month = {dec},
pages = {1--74},
title = {{Adiabatic theorems with and without spectral gap condition for non-semisimple spectral values}},
